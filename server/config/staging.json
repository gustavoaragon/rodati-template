{
  "app": {
    "port": "3000",
    "secret": "<secret>",
    "views": {
      "ws": true,
      "cache": true
    },
    "session": {
      "name": "express.sess",
      "keys": ["rodati.template"]
    }
  },
  "logger": {
    "http": "short",
    "console": {
      "level": "info",
      "silent": false,
      "colorize": true
    },
    "file": {
      "level": "debug",
      "silent": true,
      "filename": "server.log"
    }
  },
  "newrelic": {
    "enabled": true,
    "config": {
      "application_id": "rodati-template-staging",
      "app_name": [
        "rodati-template-staging"
      ],
      "license_key": "<license>",
      "logging": {
        "level": "info"
      },
      "capture_params": true
    }
  },
  "raygun": {
    "enabled": false, 
    "apiKey": "<licence>"
  }
}