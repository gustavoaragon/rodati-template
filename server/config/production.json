{
  "app": {
    "port": "3000",
    "secret": "<secret>",
    "views": {
      "ws": true,
      "cache": true
    }
  },
  "logger": {
    "http": "default",
    "console": {
      "level": "info",
      "silent": false,
      "colorize": true
    },
    "file": {
      "level": "debug",
      "silent": true,
      "filename": "server.log"
    }
  },
  "newrelic": {
    "enabled": true,
    "config": {
      "application_id": "rodati-template-production",
      "app_name": [
        "rodati-template-production"
      ],
      "license_key": "<license>",
      "logging": {
        "level": "info"
      },
      "capture_params": true
    }
  }
}